<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asjy.wtb.dao.UserDao">
	
	
	<select id="findUserByName" resultType="com.asjy.wtb.model.User">
		select
			id,
			username,
			password,
			usercname,
			addtime addTime
		from tb_user
		<where>
			<if test="username !=null and password !=null">
				username=#{username} and password=#{password}
			</if>
		</where>
	</select>
</mapper>