<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asjy.book.dao.BookDao">
	<select id="findUserById" resultType="com.asjy.book.model.User">
		select 
			no,
			id,
			password,
			level
		from
			tbl_bookuser
		where
			id=#{id}
	
	</select>
	<insert id="addUser" parameterType="com.asjy.book.model.User">
			insert into
			tbl_bookuser
			values(
			#{no},
			#{id},
			#{password},
			#{level}
			)
	
	</insert>


</mapper>